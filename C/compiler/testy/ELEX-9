Scope
	Dim a&cd As Integer 'CHYBA - identifikator obsahuje nepovoleny znak
	Dim vysl As Integer

	Print !"Zadejte cislo pro vypocet faktorialu";
	Input a
	If a < 0 Then
		Print !"\nFaktorial nelze spocitat\n";
	Else
		vysl = factorial(a)
		Print !"\nVysledek je:" ; vysl ; !"\n";
	End If
End Scope